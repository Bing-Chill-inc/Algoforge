{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"source": {
		"include": ["src"],
		"includePattern": ".+\\.js$",
		"excludePattern": "(node_modules|docs)"
	},
	"plugins": [],
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"outputSourceFiles": true
		}
	},
	"opts": {
		"destination": "docs",
		"recurse": true,
		"readme": "README.md"
	}
}
